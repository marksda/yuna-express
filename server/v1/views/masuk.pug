doctype html
html(xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US" prefix="og: http://ogp.me/ns#")
  head
    //- include includes/asynchide-script.pug
    //- include includes/gtm-script.pug
    title= title
    include includes/meta.pug
    script(type="application/ld+json").
      {
        "@context":"https://schema.org",
        "@graph":
        [
          {
            "@type":"WebSite",
            "@id":"https://cso.ddns.net"
            "url":"https://cso.ddns.net"
            "name":"CSO - Commitech Solusindo akselator bisnis anda", 
            "description":"CSO adalah perusahaan kolaborasi antara tim konsultan dan enjiner berpengalaman dalam desain dan penerapan teknologi informasi terkini. Optimasi biaya dan hasil akselerasi investasi IT merupakan prioritas capaian perusahan buat client kami.",
            "potentialAction":
            {
              "@type":"SearchAction",
              "target": "https://cso.ddns.net",
              "query-input":"required name=search_term_string"
            }
          },
          {
            "@type":"WebPage",
            "@id":"https://cso.ddns.net/masuk",
            "url":"https://cso.ddns.net/masuk",
            "inLanguage":"id-ID",
            "name":"Masuk", 
            "isPartOf":
            {
              "@id":"https://cso.ddns.net"
            },     
            "description":"Masuk ke sistem CSO"
          }
        ]
      }
    include includes/css.pug
    link(rel="stylesheet", href="/static/css/signin.cso.css")    
    script. 
      function PageTrack(key) {
        try {
          if(window.dataLayer) {
            window.dataLayer.push({'event': key});
          }
          if(window.ga) {
            window.ga('www.send', 'pageview', key);
            window.ga('testTracker.send', 'pageview', key);
          }
        }
        catch(err) {

        }
      }
  body#signinpage
    include includes/gtm-noscript.pug
    div.BaseSide.Com2
      input#browserType(type="hidden")
      div.mainpart(style="z-index:1")
        div.site-overlay
        include includes/nav.pug
        article#target
          div.contentConteiner: div.InnerPage.signinpage
            div.innerblue: div.description
              div.siginpageform: div.dataForm.SignInPanel
                h3 Masuk dengan akun anda ke sistem CSO
                div.dataItem 
                  div.input-wrapper
                    input#txtSignInEmail(type="email" tabindex="1" name="txtSignInEmail" maxlength="255" required)
                    label.dataLabel(for="txtSignInEmail") Email*
                  div#divSigninEmailError.dataFormError Error
                div.dataItem
                  div.input-wrapper
                    input#txtSignPassword(
                      type="password" 
                      tabindex="2" 
                      name="txtSignPassword" 
                      data-min="3" 
                      data-hashsize="256" 
                      data-hashiterations="10" 
                      data-hashsalt="wyAlkZTPZF5h0AXimmxgUu"
                      required)
                    label.dataLabel(for="txtSignPassword") Password*
                div.signinwithsocial
                  div.dataFormError#divSigninError Error
                  div.Progress#divSigninProgress Please wait...
                  input#signIn.button.red(type="button" value="Masuk" onclick="SignIn(); return false;" tabindex="3")
